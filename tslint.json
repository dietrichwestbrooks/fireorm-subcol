{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "member-access": false,
        "no-console": false,
        "arrow-parens": false,
        "ordered-imports": false,
        "max-classes-per-file": false,
        "variable-name": {
            "options": [
                "allow-leading-underscore"
            ]
        },
        "trailing-comma": false
    },
    "rulesDirectory": []
}